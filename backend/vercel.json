{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "redirects": [
    { "source": "/", "destination": "https://your-domain.com", "permanent": true }
  ]
  }